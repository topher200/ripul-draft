<h1>Listing next_pick_numbers</h1>

<table>
  <tr>
    <th>Gender</th>
    <th>Number</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @next_pick_numbers.each do |next_pick_number| %>
  <tr>
    <td><%= next_pick_number.gender %></td>
    <td><%= next_pick_number.number %></td>
    <td><%= link_to 'Show', next_pick_number %></td>
    <td><%= link_to 'Edit', edit_next_pick_number_path(next_pick_number) %></td>
    <td><%= link_to 'Destroy', next_pick_number, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Next pick number', new_next_pick_number_path %>
