<h1>Listing picks</h1>

<table>
  <tr>
    <th>Team</th>
    <th>Player</th>
    <th>Gender</th>
    <th>Automatic</th>
    <th>Number</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @picks.each do |pick| %>
  <tr>
    <td><%= pick.team %></td>
    <td><%= pick.player %></td>
    <td><%= pick.gender %></td>
    <td><%= pick.automatic %></td>
    <td><%= pick.number %></td>
    <td><%= link_to 'Show', pick %></td>
    <td><%= link_to 'Edit', edit_pick_path(pick) %></td>
    <td><%= link_to 'Destroy', pick, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Pick', new_pick_path %>
